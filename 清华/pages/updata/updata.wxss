/* pages/updata/updata.wxss */
page{
  width:100%;
  height:100%;
}
.wrap{
  width:100%;
  height:100%;
  background: #ccc;
  display: flex;
  flex-direction: column;
}
.list{
  width:100%;
  height:100rpx;
  display: flex;
}
.list>view{
  flex:1;
  background: #fff;
  text-align: center;
  line-height: 100rpx;
}
.wrap-one{
  width:95%;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin:40rpx auto;
  box-sizing: border-box;
  border-radius: 20rpx;
}
.top{
  width:100%;
  display: flex;
  justify-content: space-between;
  padding:20rpx;
  box-sizing: border-box;
}
.session{
  width:100%;
  display: flex;
  flex-direction: column;
  padding:20rpx;
  box-sizing: border-box;
}
.wran{
  width:100%;
  display: flex;
  flex-direction: column;
  padding:20rpx;
  box-sizing: border-box;
}
.act{
  border-bottom:2px solid #5E3862;
}
button{
  width:95%;
}
/**index.wxss**/
.boximage{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.image{
  padding: 0 20rpx;
  box-sizing: border-box;
  border-radius:30rpx;  
  width: 100%;
}
.boximg{
  width: 100%;
  height: 300rpx;
  background: #5E3862;
}
.cont{
  background: #F2F2F2;
   width: 100%;
   padding-top:150rpx; 
   box-sizing: border-box;
}
.cont .every{
  width:94%;
  height:300rpx;
  background: #fff;
  border: 1rpx solid #cdcdcd; 
  padding:0 30rpx;
  box-sizing: border-box; 
  margin: 20rpx 30rpx;
  border-radius:30rpx; 
}
.every> text{
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-weight: bolder;
}
.every>view{
  display: flex;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-top:1rpx solid rgb(196, 193, 193); 
  align-items: center;
}
.every>view>image{
 width: 75rpx;
 margin-left: 20rpx;
}
.footer{
  
  width: 100%;
  text-align: center;
  color: #5E3862;
}
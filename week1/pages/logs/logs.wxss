
.box{
  width: 100%;
  hyphens: 100%;
}
.box >text{
  font-size: 32rpx;
  font-weight: bold;
}
.h2{
   font-size: 48rpx;
   font-weight: bold;
}
.time{
  margin-top: 40rpx;
}
.time .views{
 width: 100%;
 height: 200rpx;
 display: flex;
 flex-wrap: wrap;
 padding: 0 20rpx;
 box-sizing: border-box;
}
.views view{
  width: 23%;
  display: flex;
  flex-direction: column;
  border: 1rpx solid #cdcdcd;
  border-radius:20rpx; 
  margin: 20rpx 5rpx;
  font-size: 32rpx;
}
.section{
  width: 100%;
  height: 200rpx;
  margin-top: 100rpx
}
.active{
 background:#5E3862;
 color: #fff;
  font-weight: bold;

}